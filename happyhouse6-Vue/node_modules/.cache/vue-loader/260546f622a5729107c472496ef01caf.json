  <b-card class=\"text-center mt-3\" style=\"max-width: 40rem\" align=\"left\">\n          <b-form class=\"text-left\">\n            <b-alert show variant=\"danger\" v-if=\"isRegisterError\"\n              >비어 있는 값이 있는지 확인하세요.</b-alert\n            >\n            <b-form-group label=\"이름:\" label-for=\"username\">\n              <b-form-input\n                id=\"username\"\n                v-model=\"user.username\"\n                required\n                placeholder=\"이름 입력....\"\n              ></b-form-input>\n            </b-form-group>\n            <b-form-group\n              label=\"아이디:\"\n              label-for=\"userid\"\n              @blur=\"checkDuplicate\"\n            >\n              <b-form-input\n                id=\"userid\"\n                v-model=\"user.userid\"\n                required\n                placeholder=\"아이디 입력....\"\n              ></b-form-input>\n            </b-form-group>\n            <b-form-group label=\"비밀번호:\" label-for=\"userpwd\">\n              <b-form-input\n                type=\"password\"\n                id=\"userpwd\"\n                v-model=\"user.userpwd\"\n                required\n                placeholder=\"비밀번호 입력....\"\n              ></b-form-input>\n            </b-form-group>\n            <b-form-group label=\"비밀번호 확인:\" label-for=\"pwdcheck\">\n              <b-form-input\n                type=\"password\"\n                id=\"pwdcheck\"\n                required\n                placeholder=\"비밀번호 재입력\"\n              ></b-form-input>\n            </b-form-group>\n            <b-form-group\n              label=\"이메일:\"\n              label-for=\"emailid\"\n              label-align-lg=\"left\"\n            >\n              <b-form-input\n                v-model=\"emailInfo.emailId\"\n                id=\"emailId\"\n                required\n                placeholder=\"이메일 입력...\"\n              ></b-form-input>\n              @\n              <select\n                class=\"form-control\"\n                id=\"emaildomain\"\n                name=\"emaildomain\"\n                v-model=\"emailInfo.emailDomain\"\n              >\n                <option value=\"ssafy.com\">싸피</option>\n                <option value=\"naver.com\">네이버</option>\n                <option value=\"kakao.com\">카카오</option>\n                <option value=\"google.com\">구글</option>\n              </select>\n              <input type=\"hidden\" v-model=\"user.email\" />\n            </b-form-group>\n            <b-form-group>\n              <b-button\n                type=\"button\"\n                variant=\"primary\"\n                class=\"m-1\"\n                @click=\"register\"\n                >회원가입</b-button\n              >\n              <b-button\n                type=\"button\"\n                variant=\"success\"\n                class=\"m-1\"\n                @click=\"moveToLogin\"\n                >로그인</b-button\n              >\n              <b-button\n                type=\"button\"\n                variant=\"warning\"\n                class=\"m-1\"\n                @click=\"movePage\"\n                >홈으로 가기</b-button\n              >\n            </b-form-group>\n          </b-form>\n        </b-card>\n      </b-col>\n      <b-col>\n        <img src=\"@/assets/happyhouse.png\" alt=\"뭐시기\" style=\"width: 100%\" />\n      </b-col>\n    </b-row>\n  </b-container>\n</template>\n\n<script>\nimport { mapState, mapActions } from \"vuex\";\n\nconst memberStore = \"memberStore\";\n\nexport default {\n  name: \"MemberJoin\",\n  data() {\n    return {\n      user: {\n        username: null,\n        userid: null,\n        userpwd: null,\n        email: null,\n      },\n      emailInfo: {\n        emailId: null,\n        emailDomain: null,\n      },\n    };\n  },\n  computed: {\n    ...mapState(memberStore, [\"isRegister\", \"isRegisterError\"]),\n  },\n  methods: {\n    ...mapActions(memberStore, [\"getUserInfo\", \"userRegister\"]),\n    movePage() {\n      this.$router.push({ name: \"Home\" });\n    },\n    moveToLogin() {\n      this.$router.push({ name: \"SignIn\" });\n    },\n    async register() {\n      this.user.email = \"\".concat(\n        this.emailInfo.emailId,\n        \"@\",\n        this.emailInfo.emailDomain\n      );\n      await this.userRegister(this.user);\n      console.log(this.isRegister);\n      if (this.isRegister) {\n        this.$router.push({ name: \"SignIn\" });\n      }\n    },\n    async checkDuplicate() {},\n  },\n};\n</script>\n\n<style></style>\n"]}]}