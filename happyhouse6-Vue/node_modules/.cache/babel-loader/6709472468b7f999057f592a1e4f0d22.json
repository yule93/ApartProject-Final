Z,sBAAIA,QAAQ,CAACC,IAAT,CAAcC,OAAd,KAA0B,SAA9B,EAAyC;AACvCH,oBAAAA,MAAM,CAAC,cAAD,EAAiB,IAAjB,CAAN;AACAA,oBAAAA,MAAM,CAAC,oBAAD,EAAuB,KAAvB,CAAN;AACD,mBAHD,MAGO;AACLA,oBAAAA,MAAM,CAAC,cAAD,EAAiB,KAAjB,CAAN;AACAA,oBAAAA,MAAM,CAAC,oBAAD,EAAuB,IAAvB,CAAN;AACD;AACF,iBAVe,EAWhB,YAAM,CAAE,CAXQ,CADiB;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAcpC,KAhCM;AAiCPQ,IAAAA,WAjCO,8BAiCiBJ,KAjCjB,EAiCwB;AAAA,UAAjBJ,MAAiB,SAAjBA,MAAiB;AAC7B,UAAIS,YAAY,GAAGlC,UAAU,CAAC6B,KAAD,CAA7B;AACA1B,MAAAA,QAAQ,CACN+B,YAAY,CAACC,MADP,EAEN,UAACT,QAAD,EAAc;AACZ,YAAIA,QAAQ,CAACC,IAAT,CAAcC,OAAd,KAA0B,SAA9B,EAAyC;AACvCH,UAAAA,MAAM,CAAC,eAAD,EAAkBC,QAAQ,CAACC,IAAT,CAAclB,QAAhC,CAAN;AACD,SAFD,MAEO;AACL2B,UAAAA,OAAO,CAACC,GAAR,CAAY,YAAZ;AACD;AACF,OARK,EASN,UAACC,KAAD,EAAW;AACTF,QAAAA,OAAO,CAACC,GAAR,CAAYC,KAAZ;AACD,OAXK,CAAR;AAaD;AAhDM;AArCS,CAApB;AAyFA,eAAelC,WAAf","sourcesContent":["import jwt_decode from \"jwt-decode\";\r\nimport { login, modifyMember } from \"@/api/member.js\";\r\nimport { findById } from \"../../api/member\";\r\n\r\nconst memberStore = {\r\n  namespaced: true,\r\n  state: {\r\n    isLogin: false,\r\n    isLoginError: false,\r\n    userInfo: null,\r\n    isLogout: false,\r\n    isRegister: false,\r\n    isRegisterError: false,\r\n  },\r\n  getters: {\r\n    checkUserInfo: function (state) {\r\n      return state.userInfo;\r\n    },\r\n  },\r\n  mutations: {\r\n    SET_IS_LOGIN: (state, isLogin) => {\r\n      state.isLogin = isLogin;\r\n    },\r\n    SET_IS_LOGIN_ERROR: (state, isLoginError) => {\r\n      state.isLoginError = isLoginError;\r\n    },\r\n    SET_USER_INFO: (state, userInfo) => {\r\n      state.isLogin = true;\r\n      state.userInfo = userInfo;\r\n    },\r\n    SET_LOGOUT: (state, isLogout) => {\r\n      state.isLogout = isLogout;\r\n      state.isLogin = false;\r\n    },\r\n    SET_REGISTER: (state, isRegister) => {\r\n      state.isRegister = isRegister;\r\n    },\r\n    SET_REGISTER_ERROR: (state, isRegisterError) => {\r\n      state.isRegisterError = isRegisterError;\r\n    },\r\n  },\r\n  actions: {\r\n    async userConfirm({ commit }, user) {\r\n      await login(\r\n        user,\r\n        (response) => {\r\n          if (response.data.message === \"success\") {\r\n            let token = response.data[\"access-token\"];\r\n            commit(\"SET_IS_LOGIN\", true);\r\n            commit(\"SET_IS_LOGIN_ERROR\", false);\r\n            sessionStorage.setItem(\"access-token\", token);\r\n          } else {\r\n            commit(\"SET_IS_LOGIN\", false);\r\n            commit(\"SET_IS_LOGIN_ERROR\", true);\r\n          }\r\n        },\r\n        () => {}\r\n      );\r\n    },\r\n    async userRegister({ commit }, user) {\r\n      await modifyMember(\r\n        user,\r\n        (response) => {\r\n          if (response.data.message === \"success\") {\r\n            commit(\"SET_REGISTER\", true);\r\n            commit(\"SET_REGISTER_ERROR\", false);\r\n          } else {\r\n            commit(\"SET_REGISTER\", false);\r\n            commit(\"SET_REGISTER_ERROR\", true);\r\n          }\r\n        },\r\n        () => {}\r\n      );\r\n    },\r\n    getUserInfo({ commit }, token) {\r\n      let decode_token = jwt_decode(token);\r\n      findById(\r\n        decode_token.userid,\r\n        (response) => {\r\n          if (response.data.message === \"success\") {\r\n            commit(\"SET_USER_INFO\", response.data.userInfo);\r\n          } else {\r\n            console.log(\"유저 정보 없음!!\");\r\n          }\r\n        },\r\n        (error) => {\r\n          console.log(error);\r\n        }\r\n      );\r\n    },\r\n  },\r\n};\r\n\r\nexport default memberStore;\r\n"]}]}