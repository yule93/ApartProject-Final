;AACvBlC,MAAAA,QAAQ,CACN,iBAAc;AAAA,YAAXmC,IAAW,SAAXA,IAAW;AACZ;AACAD,QAAAA,MAAM,CAAC,eAAD,EAAkBC,IAAlB,CAAN;AACD,OAJK,EAKN,UAACC,KAAD,EAAW;AACTC,QAAAA,OAAO,CAACC,GAAR,CAAYF,KAAZ;AACD,OAPK,CAAR;AASD,KAXM;AAYPG,IAAAA,QAAQ,EAAE,yBAAanB,QAAb,EAA0B;AAAA,UAAvBc,MAAuB,SAAvBA,MAAuB;AAClC,UAAMM,MAAM,GAAG;AACbtB,QAAAA,IAAI,EAAEE;AADO,OAAf;AAGAnB,MAAAA,SAAS,CACPuC,MADO,EAEP,iBAAc;AAAA,YAAXL,IAAW,SAAXA,IAAW;AACZ;AACAD,QAAAA,MAAM,CAAC,gBAAD,EAAmBC,IAAnB,CAAN;AACD,OALM,EAMP,UAACC,KAAD,EAAW;AACTC,QAAAA,OAAO,CAACC,GAAR,CAAYF,KAAZ;AACD,OARM,CAAT;AAUD,KA1BM;AA2BPK,IAAAA,YAAY,EAAE,6BAAajB,SAAb,EAA2B;AAAA,UAAxBU,MAAwB,SAAxBA,MAAwB;AACvC;AACA;AACA;AACA,UAAMQ,WAAW,GAAGC,OAAO,CAACC,GAAR,CAAYC,wBAAhC,CAJuC,CAKvC;AACA;;AACA,UAAML,MAAM,GAAG;AACbM,QAAAA,OAAO,EAAEtB,SADI;AAEbuB,QAAAA,QAAQ,EAAE,QAFG;AAGbC,QAAAA,UAAU,EAAEC,kBAAkB,CAACP,WAAD;AAHjB,OAAf;AAKAxC,MAAAA,SAAS,CACPsC,MADO,EAEP,UAACU,QAAD,EAAc;AACZ;AACAhB,QAAAA,MAAM,CAAC,gBAAD,EAAmBgB,QAAQ,CAACf,IAAT,CAAce,QAAd,CAAuBC,IAAvB,CAA4BC,KAA5B,CAAkCC,IAArD,CAAN;AACD,OALM,EAMP,UAACjB,KAAD,EAAW;AACTC,QAAAA,OAAO,CAACC,GAAR,CAAYF,KAAZ;AACD,OARM,CAAT;AAUD,KAjDM;AAkDPkB,IAAAA,WAAW,EAAE,4BAAa3C,KAAb,EAAuB;AAAA,UAApBuB,MAAoB,SAApBA,MAAoB;AAClC;AACAA,MAAAA,MAAM,CAAC,kBAAD,EAAqBvB,KAArB,CAAN;AACD;AArDM;AA7CQ,CAAnB;AAsGA,eAAeR,UAAf","sourcesContent":["import { sidoList, gugunList, houseList } from \"@/api/house.js\";\r\n\r\nconst houseStore = {\r\n  namespaced: true,\r\n  state: {\r\n    sidos: [{ value: null, text: \"선택하세요\" }],\r\n    guguns: [{ value: null, text: \"선택하세요\" }],\r\n    houses: [],\r\n    house: null,\r\n    selectedSido: null,\r\n    selectedGugun: null,\r\n  },\r\n\r\n  getters: {},\r\n\r\n  mutations: {\r\n    SET_SIDO_LIST: (state, sidos) => {\r\n      sidos.forEach((sido) => {\r\n        state.sidos.push({ value: sido.sidoCode, text: sido.sidoName });\r\n      });\r\n    },\r\n    SET_GUGUN_LIST: (state, guguns) => {\r\n      guguns.forEach((gugun) => {\r\n        state.guguns.push({ value: gugun.gugunCode, text: gugun.gugunName });\r\n      });\r\n    },\r\n    CLEAR_SIDO_LIST: (state) => {\r\n      state.sidos = [{ value: null, text: \"선택하세요\" }];\r\n    },\r\n    CLEAR_GUGUN_LIST: (state) => {\r\n      state.guguns = [{ value: null, text: \"선택하세요\" }];\r\n    },\r\n    SET_HOUSE_LIST: (state, houses) => {\r\n      //   console.log(houses);\r\n      state.houses = houses;\r\n    },\r\n    SET_DETAIL_HOUSE: (state, house) => {\r\n      state.house = house;\r\n    },\r\n    SET_DETAIL_SIDO: (state, sidoName) => {\r\n      state.selectedSido = sidoName;\r\n    },\r\n    SET_DETAIL_GUGUN: (state, gugunName) => {\r\n      state.selectedGugun = gugunName;\r\n    },\r\n  },\r\n\r\n  actions: {\r\n    getSido: ({ commit }) => {\r\n      sidoList(\r\n        ({ data }) => {\r\n          // console.log(data);\r\n          commit(\"SET_SIDO_LIST\", data);\r\n        },\r\n        (error) => {\r\n          console.log(error);\r\n        }\r\n      );\r\n    },\r\n    getGugun: ({ commit }, sidoCode) => {\r\n      const params = {\r\n        sido: sidoCode,\r\n      };\r\n      gugunList(\r\n        params,\r\n        ({ data }) => {\r\n          // console.log(commit, response);\r\n          commit(\"SET_GUGUN_LIST\", data);\r\n        },\r\n        (error) => {\r\n          console.log(error);\r\n        }\r\n      );\r\n    },\r\n    getHouseList: ({ commit }, gugunCode) => {\r\n      // vue cli enviroment variables 검색\r\n      //.env.local file 생성.\r\n      // 반드시 VUE_APP으로 시작해야 한다.\r\n      const SERVICE_KEY = process.env.VUE_APP_APT_DEAL_API_KEY;\r\n      //   const SERVICE_KEY =\r\n      //     \"9Xo0vlglWcOBGUDxH8PPbuKnlBwbWU6aO7%2Bk3FV4baF9GXok1yxIEF%2BIwr2%2B%2F%2F4oVLT8bekKU%2Bk9ztkJO0wsBw%3D%3D\";\r\n      const params = {\r\n        LAWD_CD: gugunCode,\r\n        DEAL_YMD: \"202110\",\r\n        serviceKey: decodeURIComponent(SERVICE_KEY),\r\n      };\r\n      houseList(\r\n        params,\r\n        (response) => {\r\n          //   console.log(response.data.response.body.items.item);\r\n          commit(\"SET_HOUSE_LIST\", response.data.response.body.items.item);\r\n        },\r\n        (error) => {\r\n          console.log(error);\r\n        }\r\n      );\r\n    },\r\n    detailHouse: ({ commit }, house) => {\r\n      // 나중에 house.일련번호를 이용하여 API 호출\r\n      commit(\"SET_DETAIL_HOUSE\", house);\r\n    },\r\n  },\r\n};\r\n\r\nexport default houseStore;\r\n"]}]}